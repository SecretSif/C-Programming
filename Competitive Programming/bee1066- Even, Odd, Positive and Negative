#include<stdio.h>
int main()
{
   int i,E=0,num,O=0,P=0,N=0;
   for(i=1;i<=5;i++)
   {
       scanf("%d",&num);
    if(num>0)
          P++;
    if(num%2==0)
          E++;
    if(num%2!=0)
          O++;
    if(num<0)
          N++;
    }

   printf("%d valor(es) par(es)\n",E);
   printf("%d valor(es) impar(es)\n",O);
   printf("%d valor(es) positivo(s)\n",P);
   printf("%d valor(es) negativo(s)\n",N);
   return 0;
}
